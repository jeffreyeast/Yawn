<ItemsControl x:Class="Yawn.DockTabCollection"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Yawn"
             mc:Ignorable="d"
             SizeChanged="SizeChanged_Handler">
    <ItemsControl.CommandBindings>
        <CommandBinding Command="local:CustomCommands.SelectCommand" Executed="DockTabSelected_Handler"/>
    </ItemsControl.CommandBindings>
</ItemsControl>
